{
  "name": "acp-client",
  "version": "1.0.0",
  "description": "ACP Client for communicating with claude-code-acp via JSON-RPC",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun src/index.ts",
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "acp",
    "json-rpc",
    "claude",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.5.1",
    "@zed-industries/claude-code-acp": "^0.10.6",
    "child_process": "^1.0.2",
    "claude-code-acp": "^0.1.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
